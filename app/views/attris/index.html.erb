<p id="notice"><%= notice %></p>

<h1>Attris</h1>

<table>
  <thead>
    <tr>
      <th>Attri</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attris.each do |attri| %>
      <tr>
        <td><%= attri.attri_id %></td>
        <td><%= attri.name %></td>
        <td><%= link_to 'Show', attri %></td>
        <td><%= link_to 'Edit', edit_attri_path(attri) %></td>
        <td><%= link_to 'Destroy', attri, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attri', new_attri_path %>
